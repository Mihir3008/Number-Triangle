#include<iostream>
using namespace std;

int main()
{
	int a, b, c;
	
	cout << "\nPlease Enter Three Sides of a Triangle =  ";
	cin >> a >> b >> c;
	
	if(a == b && b == c)
  	{
  		cout << "\nThis is an Equilateral Triangle";
  	}
  	else if(a == b || b == c || a == c)
    {
  		cout << "\nThis is an Isosceles Triangle";
	}
  	else
    	cout << "\nThis is a Scalene Triangle";
		
 	return 0;
}
